<!DOCTYPE html>

<title>ServiceCommand index</title>

{% block body %}
    <h1>ServiceCommand index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Type</th>
                <th>Id</th>
                <th>Data</th>
                <th>IsActive</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for service_command in service_commands %}
            <tr>
                <td>{{ service_command.type }}</td>
                <td>{{ service_command.id }}</td>
                <td>{{ service_command.data ? service_command.data|json_encode : '' }}</td>
                <td>{{ service_command.isActive ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('service-command.show', {'id': service_command.id}) }}">show</a>
                    <a href="{{ path('service-command.edit', {'id': service_command.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('service-command.new') }}">Create new</a>
{% endblock %}
