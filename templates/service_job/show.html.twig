<!DOCTYPE html>

<title>ServiceJob</title>

{% block body %}
    <h1>ServiceJob</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Result</th>
                <td>{{ service_job.result }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ service_job.id }}</td>
            </tr>
            <tr>
                <th>Data</th>
                <td>{{ service_job.data ? service_job.data|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ service_job.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ service_job.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('service-job.index') }}">back to list</a>

    <a href="{{ path('service-job.edit', {'id': service_job.id}) }}">edit</a>

    {{ include('service_job/_delete_form.html.twig') }}
{% endblock %}
